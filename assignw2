import textwrap
import sys
def wrap(filename, width):
with open(filename, &#39;r&#39;) as f:
for line in f:
print(&quot;\n&quot;.join(textwrap.wrap(line, width)))
wrap(&#39;newFile.txt&#39;,10 )

11.
def map_custom(func, lst):
return [func(x) for x in lst]
print(map_custom(lambda x: x*2, [1, 2, 3]))

12.
def filter_custom(func, lst):
return [x for x in lst if func(x)]
print(filter_custom(lambda x: x % 2 == 0, [1, 2, 3, 4, 5]))

13
def triplet(n):
result = []
for a in range(n):
for b in range(a, n):
c = a + b
if c &lt; n:
result.append((a, b, c))
return result
print(triplet(10))

14.
def mutate(word):
mutations = set()
for i in range(len(word) + 1):
for c in &#39;abcdefghijklmnopqrstuvwxyz&#39;:
mutations.add(word[:i] + c + word[i:])
for i in range(len(word)):
mutations.add(word[:i] + word[i+1:])
for i in range(len(word)):
for c in &#39;abcdefghijklmnopqrstuvwxyz&#39;:
mutations.add(word[:i] + c + word[i+1:])
for i in range(len(word) - 1):
mutations.add(word[:i] + word[i+1] + word[i] + word[i+2:])
return mutations

print(mutate(&quot;Low&quot;))

15.
def nearly_equal(a, b):
if len(a) == len(b):
count_diff = sum(1 for x, y in zip(a, b) if x != y)
return count_diff == 1
if len(a) == len(b) + 1:
for i in range(len(a)):
if a[:i] + a[i+1:] == b:
return True
if len(a) + 1 == len(b):
for i in range(len(b)):
if b[:i] + b[i+1:] == a:
return True
if len(a) == len(b) - 1:
for i in range(len(a) - 1):
if a[:i] + a[i+1] + a[i] + a[i+2:] == b:
return True
return False
print(nearly_equal(&quot;apple&quot;,&quot;mapple&quot;))

16.

from collections import Counter
import string
def count_char_frequency(filename):
with open(filename, &#39;r&#39;, encoding=&#39;utf-8&#39;) as file:
content = file.read()

char_count = Counter(content)

for char, count in char_count.items():
if char in string.printable:
print(f&quot;&#39;{char}&#39;: {count}&quot;)
return char_count
def detect_file_type(filename):
char_count = count_char_frequency(filename)

with open(filename, &#39;r&#39;, encoding=&#39;utf-8&#39;) as file:
first_line = file.readline().strip()

if first_line.startswith(&#39;#&#39;):
return &quot;C program file&quot;
elif first_line.startswith(&quot;import&quot;) or first_line.startswith(&quot;def&quot;):
return &quot;Python program file&quot;
elif all(c in string.printable for c in first_line):
return &quot;Text file&quot;
return &quot;Unknown file type&quot;
filename = &#39;newFile.txt&#39;
print(f&quot;Character Frequency in {filename}:&quot;)
count_char_frequency(filename)
print(&quot;\nFile Type Detection:&quot;)
print(detect_file_type(filename))

17.
from collections import defaultdict
def find_anagrams(words):
anagram_groups = defaultdict(list)
for word in words:
sorted_word = &#39;&#39;.join(sorted(word))
anagram_groups[sorted_word].append(word)

anagrams = [group for group in anagram_groups.values() if len(group) &gt; 1]
return anagrams
words_list = [&#39;eat&#39;, &#39;tea&#39;, &#39;ate&#39;, &#39;bat&#39;, &#39;tab&#39;, &#39;cat&#39;]
anagrams = find_anagrams(words_list)
for group in anagrams:
print(group)

