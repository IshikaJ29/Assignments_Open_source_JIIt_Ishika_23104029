friends = [&#39;Alice&#39;, &#39;Bob&#39;, &#39;Charlie&#39;, &#39;David&#39;, &#39;Eve&#39;, &#39;Frank&#39;, &#39;Grace&#39;, &#39;Hannah&#39;, &#39;Isaac&#39;, &#39;Jack&#39;]
print(f&quot;Name at the first index: {friends[1]}&quot;)
print(f&quot;Name at the last index: {friends[-1]}&quot;)
print(f&quot;Names from third to fifth index: {friends[2:5]}&quot;)
print(f&quot;Names in reverse order: {friends[::-1]}&quot;)
print(f&quot;Names from eighth to third index (in reverse): {friends[7:2:-1]}&quot;)

