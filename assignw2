3.
def duplicate(numbers):
duplicates = []
counts = {}
for num in numbers:
if num in counts:
counts[num] += 1
else:
counts[num] = 1
for num, count in counts.items():
if count &gt; 1:
duplicates.append(num)
return duplicates

my_list = [1, 2, 3, 4, 5, 2, 6, 7, 8, 9, 3]
duplicate_numbers = duplicate(my_list)
print(duplicate_numbers)

4.
def group(a,n):
res = []
for i in range(0, len(a), n):
res.append(a[i:i + n])
print(res)

a = [1, 2, 3, 4, 5, 6, 7, 8]
n = 3
group(a,n)

5.
def lensort(string_list):
return sorted(string_list, key=len)
my_list = [&quot;apple&quot;, &quot;banana&quot;, &quot;kiwi&quot;, &quot;grapefruit&quot;, &quot;date&quot;]
sorted_list = lensort(my_list)
print(sorted_list)

import os
def extsort(files):
return sorted(files, key=lambda x: os.path.splitext(x)[1])

file_list = [&#39;a.c&#39;, &#39;a.py&#39;, &#39;b.py&#39;, &#39;bar.txt&#39;, &#39;foo.txt&#39;, &#39;x.c&#39;]
sorted_files = extsort(file_list)
print(sorted_files)

