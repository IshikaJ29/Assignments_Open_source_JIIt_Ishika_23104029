def count_characters_in_file(filename):
try:
with open(filename, &#39;r&#39;, encoding=&#39;utf-8&#39;) as file:
content = file.read()
return len(content)
except FileNotFoundError:
print(f&quot;Error: File &#39;{filename}&#39; not found.&quot;)
return 0
except Exception as e:
print(f&quot;An error occurred: {e}&quot;)
return 0
filename = &quot;testfile.txt&quot; # Replace with your actual file name
char_count = count_characters_in_file(filename)
print(f&quot;Total number of characters in &#39;{filename}&#39;: {char_count}&quot;)

12.
from collections import defaultdict
def find_anagrams(words):
anagram_dict = defaultdict(list)
for word in words:
# Use the sorted letters as the key
key = &#39;&#39;.join(sorted(word))
anagram_dict[key].append(word)
# Return only groups with more than one word
return [group for group in anagram_dict.values() if len(group) &gt; 1]
word_list = [&quot;eat&quot;, &quot;tea&quot;, &quot;ate&quot;, &quot;bat&quot;, &quot;tab&quot;, &quot;tan&quot;, &quot;ant&quot;, &quot;net&quot;, &quot;ten&quot;, &quot;hello&quot;]
anagram_groups = find_anagrams(word_list)
for group in anagram_groups:
print(group)
