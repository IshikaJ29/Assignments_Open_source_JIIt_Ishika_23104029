students = {&#39;Alice&#39;: 22, &#39;Bob&#39;: 19, &#39;Charlie&#39;: 23, &#39;David&#39;: 18, &#39;Eve&#39;: 25}
for name, age in students.items():
if age &gt; 20:
print(f&quot;{name}: {age}&quot;)
students[&#39;Frank&#39;] = 30
print(students)
for name, age in students.items():
print(f&quot;{name}: {age}&quot;)
del students[&#39;Bob&#39;]
print(students)
average_age = sum(students.values()) / len(students)
print(f&quot;Average age: {average_age}&quot;)

