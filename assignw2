file_name = &quot;sample_file.txt&quot;

print(&quot;--- Writing to a new file ---&quot;)
with open(file_name, &quot;w&quot;) as file_object:
file_object.write(&quot;This is the first line.\n&quot;)
file_object.write(&quot;This is the second line.\n&quot;)
print(f&quot;Content written to &#39;{file_name}&#39;.&quot;)
print(&quot;\n--- Reading the entire file ---&quot;)
with open(file_name, &quot;r&quot;) as file_object:
content = file_object.read()
print(f&quot;File content:\n{content}&quot;)

print(&quot;\n--- Reading line by line ---&quot;)
with open(file_name, &quot;r&quot;) as file_object:
line1 = file_object.readline()
line2 = file_object.readline()

print(f&quot;First line: {line1.strip()}&quot;)
print(f&quot;Second line: {line2.strip()}&quot;)
print(&quot;\n--- Appending to the file ---&quot;)
with open(file_name, &quot;a&quot;) as file_object:
file_object.write(&quot;This line is appended.\n&quot;)
print(f&quot;Content appended to &#39;{file_name}&#39;.&quot;)
print(&quot;\n--- Reading the updated file ---&quot;)
with open(file_name, &quot;r&quot;) as file_object:
content = file_object.read()
print(f&quot;Updated file content:\n{content}&quot;)
print(&quot;\n--- Writing multiple lines using writelines() ---&quot;)
new_lines = [&quot;Line A from writelines.\n&quot;, &quot;Line B from writelines.\n&quot;]
with open(&quot;new_lines.txt&quot;, &quot;w&quot;) as file_object:
file_object.writelines(new_lines)
print(&quot;New lines written to &#39;new_lines.txt&#39;.&quot;)

print(&quot;\n--- Reading the file created with writelines() ---&quot;)
with open(&quot;new_lines.txt&quot;, &quot;r&quot;) as file_object:
content = file_object.read()
print(f&quot;Content of &#39;new_lines.txt&#39;:\n{content}&quot;)

7.
def count_file_elements(filepath):
try:
char_count = 0
word_count = 0
line_count = 0
with open(filepath, &#39;r&#39;) as file:
for line in file:
line_count += 1
char_count += len(line)
words = line.split()
word_count += len(words)

return char_count, word_count, line_count
except FileNotFoundError:
print(f&quot;Error: File not found at {filepath}&quot;)
return None

file_path = &quot;sample_data.txt&quot;
with open(file_path, &quot;w&quot;) as f:
f.write(&quot;This is line one.\n&quot;)
f.write(&quot;Second line here.\n&quot;)
f.write(&quot;And the third line with more words.&quot;)
characters, words, lines = count_file_elements(file_path)
if characters is not None:
print(f&quot;File Analysis for &#39;{file_path}&#39;:&quot;)
print(f&quot; Characters: {characters}&quot;)
print(f&quot; Words: {words}&quot;)
print(f&quot; Lines: {lines}&quot;)
non_existent_file = &quot;non_existent.txt&quot;
count_file_elements(non_existent_file)

