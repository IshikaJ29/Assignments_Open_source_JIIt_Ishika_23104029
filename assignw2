def replace_file_content(filename):
new_content = &quot;Hi, I am currently pursuing my BTech from Jaypee.&quot;
try:

with open(filename, &#39;w&#39;, encoding=&#39;utf-8&#39;) as file:
file.write(new_content)
print(f&quot;File &#39;{filename}&#39; has been updated successfully.&quot;)
except Exception as e:
print(f&quot;An error occurred: {e}&quot;)
filename = &quot;testfile.txt&quot; # Replace with your actual file name
replace_file_content(filename)

9.
# reverse.py
def print_file_in_reverse(filename):
try:
with open(filename, &#39;r&#39;, encoding=&#39;utf-8&#39;) as file:
lines = file.readlines()
for line in reversed(lines):
print(line.rstrip()) # rstrip removes trailing newline
except FileNotFoundError:
print(f&quot;Error: File &#39;{filename}&#39; not found.&quot;)
except Exception as e:
print(f&quot;An error occurred: {e}&quot;)
# Example usage
if __name__ == &quot;__main__&quot;:
filename = &quot;testfile.txt&quot; # Replace with your actual file name
print_file_in_reverse(filename)

