def get_odd_elements(numbers):
return [num for num in numbers if num % 2 != 0]
my_list = [10, 15, 22, 33, 42, 55, 60, 71]
odd_elements = get_odd_elements(my_list)
print(&quot;Odd elements:&quot;, odd_elements)

